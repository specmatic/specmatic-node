{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noEmit": true,
    },
    "files": ["global.d.ts"],
    "include": [
      "src",
      "**/*.ts"
    ]
}
