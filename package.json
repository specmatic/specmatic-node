{
  "name": "specmatic",
  "version": "0.0.1",
  "description": "Node wrapper for Specmatic",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "npm test && rimraf dist && ./node_modules/.bin/babel src --extensions '.ts,.js' --ignore '**/__tests__/*.ts' -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/znsio/specmatic-node.git"
  },
  "keywords": [
    "qontract",
    "contract",
    "testing",
    "specmatic"
  ],
  "author": "Sahil Sharma (sahilaug@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/znsio/specmatic-node/issues"
  },
  "homepage": "https://github.com/znsio/specmatic-node#readme",
  "bin": {
    "specmatic": "dist/bin/index.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  },
  "specmatic": {
    "logLevel": "trace"
  },
  "dependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.6",
    "@babel/preset-env": "^7.22.6",
    "@babel/preset-typescript": "^7.22.5",
    "@types/node": "^20.3.3",
    "@types/node-fetch": "^2.6.4",
    "@types/yargs": "^17.0.24",
    "exec-sh": "^0.4.0",
    "express-list-endpoints": "^6.0.0",
    "fast-xml-parser": "^4.2.5",
    "node-fetch": "^2.6.11",
    "rimraf": "^5.0.1",
    "terminate": "^2.6.1",
    "winston": "^3.9.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/jest": "^29.5.2",
    "@types/supertest": "^2.0.12",
    "express": "^4.18.2",
    "jest": "^29.6.0",
    "jest-extended": "^4.0.0",
    "jest-mock-extended": "^3.0.4",
    "supertest": "^6.3.3"
  }
}
