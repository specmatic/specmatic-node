{
  "name": "specmatic",
  "version": "0.67.3",
  "description": "Node wrapper for Specmatic",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "npm test && rimraf dist && ./node_modules/.bin/babel src --extensions '.ts,.js' --ignore '**/__tests__/*.ts' -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/znsio/specmatic-node.git"
  },
  "keywords": [
    "qontract",
    "contract",
    "testing",
    "specmatic"
  ],
  "author": "Sahil Sharma (sahilaug@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/znsio/specmatic-node/issues"
  },
  "homepage": "https://github.com/znsio/specmatic-node#readme",
  "bin": {
    "specmatic": "dist/bin/index.js"
  },
  "jest": {
    "setupFilesAfterEnv": ["jest-extended/all"]
  },
  "specmatic": {
    "logLevel": "debug"
  },
  "dependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/node": "^20.2.3",
    "@types/node-fetch": "^2.6.4",
    "@types/yargs": "^17.0.24",
    "exec-sh": "^0.4.0",
    "fast-xml-parser": "^4.2.2",
    "node-fetch": "^2.6.11",
    "rimraf": "^5.0.1",
    "winston": "^3.8.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-mock-extended": "^3.0.4"
  }
}
